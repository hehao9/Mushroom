{
  "manifest_version": 2,
  "name": "image",
  "version": "1.0.0",
  "description": "简单、快速地收藏你喜欢的图片。",
  "icons": {
    "84": "mushroom.png"
  },
  "browser_action": {
    "default_icon": "mushroom.png",
    "default_title": "我的插件",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}